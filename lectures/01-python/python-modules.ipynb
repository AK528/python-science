      "t = profile.timer(\"main loop\")\n",
      "t.begin()\n",
      "\n",
      "n = 0\n",
      "sum = 0.0\n",
      "while (n < 1000):\n",
      "    sum += n**2\n",
      "    n += 1\n",
      "\n",
      "\n",
      "t.end()\n",
      "profile.timeReport()\n",
      "\n",
      "print sum"
     ],
